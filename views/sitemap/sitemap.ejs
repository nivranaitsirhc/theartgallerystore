<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">

<!-- landing -->
	<url>
		<loc>https://<%=sitemap.host%></loc>
		<lastmod>2020-10-12</lastmod>
		<!-- <changefreq>yearly</changefreq> -->
		<priority>0.4</priority>
	</url>
<!-- about	 -->
	<url>
		<loc>https://<%=sitemap.host%>/about</loc>
		<lastmod>2020-10-12</lastmod>
		<!-- <changefreq>yearly</changefreq> -->
		<priority>0.4</priority>
	</url>
<!-- profile -->
	<url>
		<loc>https://<%=sitemap.host%>/profile</loc>
		<lastmod>2020-10-12</lastmod>
		<!-- <changefreq>yearly</changefreq> -->
		<priority>0.4</priority>
	</url>
<!-- sign-in -->
	<url>
		<loc>https://<%=sitemap.host%>/signin</loc>
		<lastmod>2020-10-12</lastmod>
		<!-- <changefreq>yearly</changefreq> -->
		<priority>0.4</priority>
	</url>
<!-- signup -->
	<url>
		<loc>https://<%=sitemap.host%>/signup</loc>
		<lastmod>2020-10-12</lastmod>
		<!-- <changefreq>yearly</changefreq> -->
		<priority>0.4</priority>
	</url>
<!-- reset-password -->
	<url>
		<loc>https://<%=sitemap.host%>/reset</loc>
		<lastmod>2020-10-12</lastmod>
		<!-- <changefreq>yearly</changefreq> -->
		<priority>0.4</priority>
	</url>
<!-- posts -->
<%for(let x of sitemap.artgallery){%>
	<url>
		<loc>https://<%=sitemap.host%>/artgallery/<%=x.loc%></loc>
		<lastmod><%=x.lastmod%></lastmod>
		<changefreq><%=x.changefreq%></changefreq>
		<priority><%=x.priority%></priority>
	</url>
<%}%>
</urlset>