<!-- posts -->
<%sitemap.artgallery.forEach(x=>{%>
	<url>
		<loc>https://<%=sitemap.host%>/artgallery/<%=x.loc%></loc>
		<lastmod><%=x.lastmod%></lastmod>
		<changefreq><%=x.changefreq%></changefreq>
		<priority><%=x.priority%></priority>
	</url>
<%})%>
</urlset>